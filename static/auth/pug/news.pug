extends header.pug

block content

	main.main-news
		.popup.popup_add_article
			.popup__bgd
			.popup__content
				h2 попап
				form(action="#")
					input(type="text" placeholder="Название объявления")
					input(type="text" placeholder="Имя")
		.sides
			.main-news__block1.main-news__block1_full
				h2 Форум
				.main-news__inner_nothing
					p.subtitle Здесь студенты могут задавать вопросы, получать ответы и общаться друг с другом.
					img(data-src="img/news1.png" alt="Photo" width="229")
					button.btn-v3 Задать вопрос
					small Вопросов пока нет
				.main-news__inner_full
					.q__block
						form(action="#").make-q
							input(type="text" placeholder="Задайте свой вопрос здесь")
							button(type="submit") Задайть вопрос
						.all-q
							.select
								.select__head
									span Все вопросы
									<svg width="12" height="7" viewBox="0 0 12 7" fill="none" xmlns="http://www.w3.org/2000/svg">
										<path d="M10.9167 1.00007L5.96074 5.96081L1 1.00488" stroke="#A5E7E3" stroke-linecap="round" stroke-linejoin="round"/>
									</svg>
								.select__body
									ul
										li 1 вопрос
										li 2 вопрос
										li 3 вопрос
										li 4 вопрос
										li 5 вопрос
										li 6 вопрос
										li 7 вопрос
										li 8 вопрос
										li 9 вопрос
										li 10 вопрос
					.questions__block
						button.view-new-q Показать <span>10</span> новых вопросов
